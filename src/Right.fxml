<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets ?>
<?import javafx.scene.layout.StackPane?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="ControllerRight">
    <children>
        <BorderPane VBox.vgrow="ALWAYS" maxHeight="Infinity">
            <padding><Insets top="25" right="25" bottom="25" left="25"/></padding>
            <top>
                <StackPane fx:id="CommandPane" />
            </top>
            <bottom>
                <Button text="Save ..." BorderPane.alignment="CENTER" onMouseClicked="#saveScreenshot" />
            </bottom>
        </BorderPane>
    </children>
</VBox>